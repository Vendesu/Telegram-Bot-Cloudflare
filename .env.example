# Bot Cloudflare Pro - Environment Configuration
# Copy this file to .env and fill in your values

# Telegram Bot Configuration
TELEGRAM_TOKEN=your_telegram_bot_token_here

# Domain Configuration
DEFAULT_DOMAIN=yourdomain.com

# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id_here

# Channel Configuration
CHANNEL_ID=@your_channel_username
DONATION_CHANNEL_ID=@your_donation_channel_username

# Security Configuration (Auto-generated, don't change)
ENCRYPTION_KEY=auto_generated_encryption_key_here

# Optional: Custom Settings
# BOT_NAME=Cloudflare Pro Bot
# LOG_LEVEL=INFO
# BACKUP_INTERVAL=24
# STATUS_UPDATE_INTERVAL=4